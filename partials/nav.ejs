<header class="header">
    <div class="header__wrapper">
        <a href="/" class="header__title-wrapper js-smooth-scroll">
            <div class="header__title-main"> Lead Together </div>
        </a>
        <div class="header__social-icons" style="margin-top: 11px">
            <% if (pack.language === '0') { %>
                <a href="language?id=1&url=<%= pack.url %>" style="font-size: 22px"><i class="fa fa-language"></i> Ingl√©s</a>
            <% } else if (pack.language === '1') { %>
                <a href="language?id=0&url=<%= pack.url %>" style="font-size: 22px"><i class="fa fa-language"></i> Spanish</a>
            <%}%>
            <% if (typeof session.login === 'undefined') { %>
            <a style="font-size:22px;font-weight: 500;" href="/login"><i class="fa fa-sign-in"></i> <%= pack.nav[0][pack.language]%></a>
            <% } else { %>
            <a style="font-size:22px;font-weight: 500;" href="/logout"><i class="fa fa-sign-out"></i> <%= pack.nav[1][pack.language]%></a>
            <% } %>
        </div>
    </div>
</header>